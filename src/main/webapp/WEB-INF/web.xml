<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="https://jakarta.ee/xml/ns/jakartaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_5_0.xsd"
	version="5.0" metadata-complete="true">
	<!--Start
	of JSP mapping-->

	<servlet>
		<servlet-name>Home</servlet-name>
		<jsp-file>/presentation/views/index-5.jsp</jsp-file>
	</servlet>
	<servlet>
		<servlet-name>About</servlet-name>
		<jsp-file>/presentation/views/about-2.jsp</jsp-file>
	</servlet>

	<servlet>
		<servlet-name>contact</servlet-name>
		<jsp-file>/presentation/views/contact-2.jsp</jsp-file>
	</servlet>
	<servlet>
		<servlet-name>checkout</servlet-name>
		<jsp-file>/presentation/views/Checkout.jsp</jsp-file>
	</servlet>
	<servlet>
		<servlet-name>wishlist</servlet-name>
		<jsp-file>/presentation/views/wishlist.jsp</jsp-file>
	</servlet>
	<servlet>
		<servlet-name>cart</servlet-name>
		<jsp-file>/presentation/views/cart.jsp</jsp-file>
	</servlet>
	<servlet>
		<servlet-name>login</servlet-name>
		<jsp-file>/presentation/views/login.jsp</jsp-file>
	</servlet>


	<!-- Admin Part -->
	<servlet>
		<servlet-name>adminProducts</servlet-name>
		<servlet-class>gov.iti.jets.controllers.Admin.ProductListServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>adminEditProduct</servlet-name>
		<servlet-class>gov.iti.jets.controllers.Admin.EditProductServlet</servlet-class>
	</servlet>


	<servlet>
		<servlet-name>adminDeleteUser</servlet-name>
		<servlet-class>gov.iti.jets.controllers.Admin.DeleteUserServlet</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>EditProduct</servlet-name>
		<servlet-class>gov.iti.jets.controllers.Admin.EditProductServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>Add-Product-Page</servlet-name>
		<jsp-file>/presentation/views/Admin/addProduct.jsp</jsp-file>
	</servlet>
	<servlet>
		<servlet-name>Products-list</servlet-name>
		<jsp-file>/presentation/views/Admin/productsList.jsp</jsp-file>
	</servlet>
	<servlet>
		<servlet-name>Admin-Home</servlet-name>
		<jsp-file>/presentation/views/Admin/HomePage.jsp</jsp-file>
	</servlet>
	<!-- End Adim Part -->
	<servlet>
		<servlet-name>Users-List</servlet-name>
		<jsp-file>/presentation/views/Admin/usersList.jsp</jsp-file>
	</servlet>

	<servlet>
		<servlet-name>Add-Category-Page</servlet-name>
		<jsp-file>/presentation/views/Admin/addCategory.jsp</jsp-file>
	</servlet>
	<!---->

	<servlet-mapping>
		<servlet-name>adminEditProduct</servlet-name>
		<url-pattern>/admin/edit</url-pattern>
	</servlet-mapping>


	<servlet-mapping>
		<servlet-name>adminDeleteUser</servlet-name>
		<url-pattern>/Delete-User</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>EditProduct</servlet-name>
		<url-pattern>/edit-product</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>Home</servlet-name>
		<url-pattern>/home</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>About</servlet-name>
		<url-pattern>/about</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>contact</servlet-name>
		<url-pattern>/contact</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>wishlist</servlet-name>
		<url-pattern>/wishlist</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>cart</servlet-name>
		<url-pattern>/cart</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Add-Product-Page</servlet-name>
