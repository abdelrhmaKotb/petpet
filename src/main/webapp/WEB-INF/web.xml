		<servlet-name>EditeProduct</servlet-name>
		<url-pattern>/Edit-Product</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>DeleteProduct</servlet-name>
		<servlet-class>gov.iti.jets.controllers.Admin.DeleteProductServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DeleteProduct</servlet-name>
		<url-pattern>/Delete-Product</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>All-Products</servlet-name>
		<servlet-class>gov.iti.jets.controllers.AllProduct</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>All-Products</servlet-name>
		<url-pattern>/Shop</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>adminShowProduct</servlet-name>
		<servlet-class>gov.iti.jets.controllers.Admin.ShowProductServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>adminShowProduct</servlet-name>
		<url-pattern>/admin/show</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Product</servlet-name>
		<servlet-class>gov.iti.jets.controllers.ProductServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Product</servlet-name>
		<url-pattern>/product</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Review-Customer-Profile</servlet-name>
		<servlet-class>gov.iti.jets.controllers.Admin.ReviewCustomerServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Review-Customer-Profile</servlet-name>
		<url-pattern>/review-customers</url-pattern>
	</servlet-mapping>
