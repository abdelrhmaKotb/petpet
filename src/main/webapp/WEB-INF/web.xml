		<servlet-name>CheckCountry</servlet-name>
		<servlet-class>gov.iti.jets.controllers.CheckCountryServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CheckCountry</servlet-name>
		<url-pattern>/checkcountry</url-pattern>
	</servlet-mapping>
	<!-- Welcome List-->

	<filter>
		<filter-name>AddHeaderFilter</filter-name>
		<filter-class>gov.iti.jets.filters.AddHeaderFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>AddHeaderFilter</filter-name>
		<url-pattern>/*</url-pattern>
		<!-- <servlet-name> MyServlet </servlet-name> -->
	</filter-mapping>

	<welcome-file-list>
		<welcome-file>
			presentation/views/index-5.jsp
		</welcome-file>
	</welcome-file-list>

</web-app>