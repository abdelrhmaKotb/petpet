	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Products-list</servlet-name>
		<url-pattern>/Products-List</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Admin-Home</servlet-name>
		<url-pattern>/Admin-Home</url-pattern>
	</servlet-mapping>
	<!--end
	of JSP mapping-->
	<servlet-mapping>
		<servlet-name>Users-List</servlet-name>
		<url-pattern>/Users-List</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>AddProduct</servlet-name>
		<servlet-class>gov.iti.jets.controllers.Admin.AddProductServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AddProduct</servlet-name>
		<url-pattern>/add-product</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>RegisterService</servlet-name>
		<servlet-class>gov.iti.jets.controllers.RegisterServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RegisterService</servlet-name>
		<url-pattern>/register</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>loginserv</servlet-name>
		<servlet-class>gov.iti.jets.controllers.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>loginserv</servlet-name>
		<url-pattern>/login_</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>Add-Category-Page</servlet-name>
		<url-pattern>/Add-Category-Page</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>addToCart</servlet-name>
		<servlet-class>gov.iti.jets.controllers.AddToCartServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>addToCart</servlet-name>
		<url-pattern>/addToCart</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>showToCart</servlet-name>
		<servlet-class>gov.iti.jets.controllers.AddToCartServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>showToCart</servlet-name>
		<url-pattern>/cartt</url-pattern>
	</servlet-mapping>
	<!--end
	of JSP mapping-->
	<servlet>
		<servlet-name>Products</servlet-name>
		<servlet-class>gov.iti.jets.controllers.Admin.ProductListServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Products</servlet-name>
		<url-pattern>/Products</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>AddCategory</servlet-name>
		<servlet-class>gov.iti.jets.controllers.Admin.AddCategoryServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AddCategory</servlet-name>
		<url-pattern>/add-category</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>EditeProduct</servlet-name>
		<servlet-class>gov.iti.jets.controllers.Admin.EditProductServlet</servlet-class>
	</servlet>
